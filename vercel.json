{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1", "continue": true },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ]
}
